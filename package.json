{
  "name": "vscode-open-in-ship",
  "displayName": "Open in Ship",
  "description": "Adds a command for opening the current project in Ship.",
  "icon": "resources/logo-128x128.png",
  "version": "1.0.0",
  "license": "MIT",
  "main": "out/src/extension.js",
  "publisher": "fabiospampinato",
  "activationEvents": [
    "onCommand:openInShip.open"
  ],
  "contributes": {
    "commands": [
      {
        "command": "openInShip.open",
        "title": "Open in Ship"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "publish": "vsce publish && npm publish",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/vscode-open-in-ship/issues"
  },
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/vscode-open-in-ship.git"
  },
  "engines": {
    "vscode": "^1.15.0"
  },
  "keywords": [
    "vscode",
    "vsc",
    "extension",
    "open in",
    "ship"
  ],
  "categories": [
    "Other"
  ],
  "dependencies": {
    "absolute": "0.0.1",
    "applescript": "^1.0.0",
    "find-up": "^2.1.0",
    "lodash": "^4.17.4",
    "pify": "^3.0.0",
    "simple-git": "^1.95.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.40",
    "tslint": "^4.4.2",
    "typescript": "~2.2.0",
    "vscode": "^1.1.4"
  }
}
